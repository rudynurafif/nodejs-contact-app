<h1>Web Server ExpressJS</h1>
<h2>Halo <%= nama %></h2>

<h4>Daftar Mahasiswa</h4>
<% if (mahasiswa.length < 1) { %>
  <p>Daftar Mahasiswa Masing Kosong</p>
<% } %>
<% mahasiswa.forEach(mhs => { %> 
  <ul>
    <li>Nama : <%= mhs.nama %> </li>
    <li>Email : <%= mhs.email %> </li>
  </ul>
  <% }) %>

